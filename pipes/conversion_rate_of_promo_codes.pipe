NODE promo_usage
SQL >

    SELECT PromoCodeUsed, COUNT(*) as Used_Count
    FROM google_sheets
    WHERE PromoCodeUsed = 'Yes'
    GROUP BY PromoCodeUsed



NODE total_purchases
SQL >

    SELECT COUNT(*) as Total_Count FROM google_sheets



NODE conversion_rate_of_promo_codes_2
SQL >

    SELECT
        PromoCodeUsed,
        Used_Count,
        (Used_Count / Total_Count * 100) as Conversion_Rate
    FROM promo_usage, total_purchases


